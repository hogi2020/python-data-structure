    "def backtrack(n, path=[]):\n",
    "   if len(path) == n:\n",
    "      print(path)\n",
    "      return\n",
    "   \n",
    "   for i in range(1, n+1):\n",
    "      if i not in path:\n",
    "         backtrack(n, path + [i])"
   ]
